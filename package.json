{
  "name": "qcode",
  "version": "1.0.0",
  "description": "Enterprise-grade TypeScript-based AI coding assistant with zero API costs and full privacy control",
  "main": "dist/index.js",
  "bin": {
    "qcode": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/cli.js",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "tc": "tsc --noEmit",
    "validate": "npm run tc && npm run format:check && npm run lint",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "dev:cli": "ts-node src/cli.ts"
  },
  "keywords": [
    "ai",
    "coding-assistant",
    "typescript",
    "cli",
    "ollama",
    "mcp",
    "privacy"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "@types/diff": "^7.0.2",
    "@types/node": "^20.10.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "diff": "^8.0.2",
    "fast-glob": "^3.3.2",
    "is-path-in-cwd": "^4.0.0",
    "micromatch": "^4.0.5",
    "node-diff3": "^3.1.1",
    "ollama": "^0.5.16",
    "pathe": "^2.0.3",
    "sanitize-filename": "^1.6.3",
    "shell-escape": "^0.2.0",
    "typescript": "^5.3.0",
    "winston": "^3.17.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/micromatch": "^4.0.6",
    "@types/nock": "^10.0.3",
    "@types/shell-escape": "^0.2.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nock": "^14.0.5",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
